{
  "name": "adobe-cloud-manager-menubar",
  "version": "0.1.0",
  "description": "Adobe Cloud Manager Menubar",
  "main": "main.js",
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "scripts": {
    "postinstall": "electron-rebuild",
    "precommit": "lint-staged",
    "prettier": "prettier --vue-indent-script-and-style --write ./app/**.{js,ts,vue} ./app/**/**.{js,ts,vue}  ",
    "pre-build": "node pre-build.js",
    "parcel:build": "parcel build app/main.js",
    "parcel:watch": "parcel watch app/main.js",
    "pre-electron": "npm run pre-build && npm run parcel:build",
    "electron-only": "electron -help",
    "electron": "npm run pre-electron && electron .",
    "electron:watch": "npm run pre-electron && concurrently \"npm:electron-only\" \"npm:parcel:watch\"",
    "electron:watch:debug": "DEBUG_E=true npm run electron:watch",
    "start": "npm run electron",
    "pack": "npm run pre-electron && electron-packager . \"Adobe Cloud Manager Menubar\" --platform=darwin --arch=x64 --overwrite --icon=icon/icon --ignore=\".*(cache|vscode|app).*\"",
    "generate-client": "swaxios -i ./app/generated/api.yml -o ./app/generated/api"
  },
  "lint-staged": {
    "*.{js,json,css,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.0",
    "concurrently": "^5.0.1",
    "cssnano": "^4.1.10",
    "electron": "^7.1.4",
    "electron-packager": "^14.1.1",
    "electron-rebuild": "^1.8.8",
    "fs-extra": "^8.1.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "1.19.1",
    "swaxios": "^0.1.3",
    "typescript": "^3.7.3",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@adobe/coral-spectrum": "^4.0.0",
    "@adobe/jwt-auth": "^0.3.0",
    "@spectrum-css/breadcrumb": "^2.0.3",
    "axios": "^0.19.0",
    "electron-store": "^5.1.0",
    "keytar": "^5.0.0",
    "menubar": "^7.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  }
}
