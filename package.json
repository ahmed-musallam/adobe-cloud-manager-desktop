{
  "name": "adobe-cloud-manager-desktop",
  "version": "0.1.0",
  "description": "Adobe Cloud Manager Desktop",
  "main": "main.js",
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "scripts": {
    "postinstall": "electron-rebuild",
    "precommit": "lint-staged",
    "prettier": "prettier --vue-indent-script-and-style --write ./app/**.{js,ts,vue} ./app/**/**.{js,ts,vue}  ",
    "start": "gulp",
    "generate-client": "swaxios -i ./app/generated/api.yml -o ./app/generated/api",
    "gulp": "gulp"
  },
  "lint-staged": {
    "./app/*.{js,json,css,vue}": [
      "prettier --vue-indent-script-and-styl --writee",
      "git add"
    ]
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.1",
    "concurrently": "^5.1.0",
    "cssnano": "^4.1.10",
    "electron": "^8.0.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.2.0",
    "electron-rebuild": "^1.10.0",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "1.19.1",
    "sass": "^1.25.0",
    "swaxios": "^0.1.3",
    "typescript": "^3.7.5",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@adobe/coral-spectrum": "^4.1.0",
    "@adobe/jwt-auth": "^0.3.1",
    "@spectrum-css/breadcrumb": "^2.0.3",
    "axios": "^0.19.2",
    "clusterize.js": "^0.18.1",
    "deep-rename-keys": "^0.2.1",
    "keytar": "^5.0.0",
    "menubar": "^8.0.0",
    "uritemplate": "^0.3.4",
    "vue": "^2.6.10",
    "vue-router": "^3.1.5"
  }
}
