{
  "name": "adobe-cloud-manager-desktop",
  "version": "1.0.0-alpha.2",
  "description": "Adobe Cloud Manager Desktop",
  "main": "main.js",
  "author": "Ahmed Musallam <ahmed@ahmedmusallam.com> (https://ahmedmusallam.com)",
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "scripts": {
    "postinstall": "electron-rebuild",
    "prettier": "prettier --write ./app/**.{js,ts,vue} ./app/**/**.{js,ts,vue}",
    "start": "gulp",
    "generate-client": "swaxios -i ./app/generated/api.yml -o ./app/generated/api",
    "gulp": "gulp"
  },
  "lint-staged": {
    "./app/*.{js,json,css,vue}": [
      "npm run prettier",
      "git add"
    ]
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.1",
    "concurrently": "^5.1.0",
    "cssnano": "^4.1.10",
    "electron": "^13.6.6",
    "electron-installer-dmg": "^3.0.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.10.1",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "1.19.1",
    "sass": "^1.26.3",
    "swaxios": "^0.1.3",
    "typescript": "3.8.3",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@adobe/coral-spectrum": "^4.3.2",
    "@adobe/jwt-auth": "^0.3.1",
    "@spectrum-css/breadcrumb": "^2.0.5",
    "axios": "^0.19.2",
    "clusterize.js": "^0.18.1",
    "deep-rename-keys": "^0.2.1",
    "electron-store": "^5.1.1",
    "keytar": "5.1.0",
    "menubar": "^8.0.1",
    "uritemplate": "^0.3.4",
    "vue": "^2.6.10",
    "vue-router": "^3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
